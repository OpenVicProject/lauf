    [ sint -42; $lauf.int.sabs_flag; ] uint 0; $lauf.test.assert_eq; sint 42; $lauf.test.assert_eq;
    [ uint 0x8000_0000_0000_0000; $lauf.int.sabs_flag; ] uint 1; $lauf.test.assert_eq; uint 0x8000_0000_0000_0000; $lauf.test.assert_eq;

