function @uabs() {
    [ sint 11; $lauf.int.sabs_wrap; ] uint 11; $lauf.test.assert_eq;
    [ sint -42; $lauf.int.sabs_wrap; ] uint 42; $lauf.test.assert_eq;
    [ uint 0x8000_0000_0000_0000; $lauf.int.sabs_wrap; ] uint 0x8000_0000_0000_0000; $lauf.test.assert_eq;

    return;
}

function @main() {
    call @sadd_flag; call @sadd_wrap; call @sadd_sat; call @sadd_panic;
    call @ssub_flag; call @ssub_wrap; call @ssub_sat; call @ssub_panic;
    call @smul_flag; call @smul_wrap; call @smul_sat; call @smul_panic;

    call @uadd_flag; call @uadd_wrap; call @uadd_sat; call @sadd_panic;
    call @usub_flag; call @usub_wrap; call @usub_sat; call @ssub_panic;
    call @umul_flag; call @umul_wrap; call @umul_sat; call @smul_sat;

    call @sdiv_flag; call @sdiv_wrap; call @sdiv_sat; call @sdiv_panic;
    call @udiv;

    call @srem;
    call @urem;

    call @scmp;
    call @ucmp;

    call @stou_flag; call @stou_wrap; call @stou_sat; call @stou_panic;
    call @utos_flag; call @utos_wrap; call @utos_sat; call @utos_panic;
    call @sabs_flag; call @sabs_wrap; call @sabs_sat; call @sabs_panic;
    call @uabs;

    return;
}

